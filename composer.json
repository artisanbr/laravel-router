{
    "name": "artisan-labs/laravel-router",
    "version": "dev-stable",
    "description": "Laravel Controller phpDOC Route Generator",
    "keywords": [
        "laravel",
        "route",
        "controller",
        "generate",
        "phpdoc"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Renalcio Carlos",
            "email": "renalcio.c@gmail.com",
            "homepage": "https://github.com/renalcio",
            "role": "Co-founder & Developer"
        }
    ],
    "require": {
        "php": ">=7",
        "laravel/framework": "*",
        "barryvdh/reflection-docblock": "^2.0.6"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArtisanLabs\\LaravelRouter\\RouterServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "ArtisanLabs\\LaravelRouter\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
